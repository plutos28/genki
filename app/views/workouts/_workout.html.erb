<turbo-stream action="append" target="workouts">
  <template>
    <turbo-frame class="w-72 h-14 py-6 px-4 flex items-center justify-between rounded-lg border border-gray-500 hover:shadow-[0px_0px_0px_1px_rgba(0,0,0,0.06),0px_1px_1px_-0.5px_rgba(0,0,0,0.06),0px_3px_3px_-1.5px_rgba(0,0,0,0.06),_0px_6px_6px_-3px_rgba(0,0,0,0.06),0px_12px_12px_-6px_rgba(0,0,0,0.06),0px_24px_24px_-12px_rgba(0,0,0,0.06)]">
      <%= link_to "Workout #{workout[:id]}",
        workout_path(workout[:id]),
        data: { turbo_frame: 'show_workout_plan'},
      class: "" %>
      <%= link_to 'Delete', workout_path(workout), method: :delete, class: 'btn btn-danger', class: "bg-red-400 rounded-md text-white py-2 px-4" %>
    </turbo-frame>
  </template>
</turbo-stream>